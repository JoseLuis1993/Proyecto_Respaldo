@model IEnumerable < ProyectoRespaldo.Models.Informacion>
@{
    ViewBag.Title = "Index_MVirtual";
}
<h2>Index_Prueba</h2><br />

    <table id="example" class="table table-striped table-bordered dt-responsive nowrap" border="0" cellspacing="0" width="100%">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.nombre)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.apellido)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.edad)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.status)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.fecha_nac)
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
            {
            <tr>
                <th>
                    @Html.DisplayFor(modelItem => item.nombre)
                </th>
                <th>
                    @Html.DisplayFor(modelItem => item.apellido)
                </th>
                <th>
                    @Html.DisplayFor(modelItem => item.edad)
                </th>
                <th>
                    @Html.DisplayFor(modelItem => item.status)
                </th>
                <th>
                    @Html.DisplayFor(modelItem => item.fecha_nac)
                </th>
            </tr>
        }
    </tbody>
</table>


 @section scripts
{
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/datatables")
    <script type="text/javascript">
   
        $(document).ready(function () {
            $('#example').DataTable();
        });
    </script>
}


